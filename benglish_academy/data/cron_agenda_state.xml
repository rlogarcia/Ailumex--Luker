<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- ===================================================================
             CRON JOB: Actualizar Estado de Agendas a Ejecutadas
             Actualiza automÃ¡ticamente el estado de las agendas publicadas
             cuya fecha de fin ya ha pasado al estado "Ejecutada"
             Ejecutar cada 1 hora
        =================================================================== -->
        <record id="ir_cron_update_executed_agendas" model="ir.cron">
            <field name="name">Actualizar Horarios Ejecutados</field>
            <field name="model_id" ref="model_benglish_academic_agenda"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_executed_agendas()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active" eval="True"/>
            <field name="priority">5</field>
            <field name="user_id" ref="base.user_admin"/>
        </record>

    </data>
</odoo>
