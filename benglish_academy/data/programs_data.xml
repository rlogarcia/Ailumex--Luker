        <!-- Programa Benglish -->
        <record id="program_benglish" model="benglish.program">
            <field name="name">Benglish</field>
            <field name="code">BE-PROG</field>
            <field name="program_type">benglish</field>
            <field name="sequence">20</field>
            <field name="description">Programa de inglés para adultos diseñado para desarrollar
                competencias comunicativas desde nivel básico hasta avanzado.</field>
            <field name="active" eval="True" />
        </record>

    </data>
