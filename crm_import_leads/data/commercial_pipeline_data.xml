<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- HU-CRM-04: Pipeline Comercial -->

        <record id="crm_team_comercial" model="crm.team">
            <field name="name">Comercial</field>
            <field name="sequence">6</field>
            <field name="use_leads">True</field>
            <field name="use_opportunities">True</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="crm_stage_comercial_evaluacion" model="crm.stage">
            <field name="name">En evaluación</field>
            <field name="sequence">1</field>
            <field name="team_id" ref="crm_team_comercial"/>
            <field name="fold">False</field>
            <field name="is_won">False</field>
        </record>

        <record id="crm_stage_comercial_reprogramado" model="crm.stage">
            <field name="name">Reprogramado</field>
            <field name="sequence">2</field>
            <field name="team_id" ref="crm_team_comercial"/>
            <field name="fold">False</field>
            <field name="is_won">False</field>
        </record>

        <record id="crm_stage_comercial_no_show" model="crm.stage">
            <field name="name">Incumplió cita</field>
            <field name="sequence">3</field>
            <field name="team_id" ref="crm_team_comercial"/>
            <field name="fold">False</field>
            <field name="is_won">False</field>
        </record>

        <record id="crm_stage_comercial_reprobado" model="crm.stage">
            <field name="name">Reprobado</field>
            <field name="sequence">4</field>
            <field name="team_id" ref="crm_team_comercial"/>
            <field name="fold">True</field>
            <field name="is_won">False</field>
        </record>

        <record id="crm_stage_comercial_pago_parcial" model="crm.stage">
            <field name="name">Pago parcial</field>
            <field name="sequence">5</field>
            <field name="team_id" ref="crm_team_comercial"/>
            <field name="fold">False</field>
            <field name="is_won">False</field>
        </record>

        <record id="crm_stage_comercial_matriculado" model="crm.stage">
            <field name="name">Matriculado</field>
            <field name="sequence">6</field>
            <field name="team_id" ref="crm_team_comercial"/>
            <field name="fold">True</field>
            <field name="is_won">True</field>
        </record>
    </data>
</odoo>
